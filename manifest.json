{
  "name": "Backblaze",
  "description": "A Chrome Extension to facilitate Multithreaded downloads from B2",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "backblaze.html",
    "default_icon": {
      "16": "/images/backblaze16.png",
      "32": "/images/backblaze32.png",
      "48": "/images/backblaze48.png",
      "128": "/images/backblaze128.png"
    }
  },
  "icons": {
    "16": "/images/backblaze16.png",
    "32": "/images/backblaze32.png",
    "48": "/images/backblaze48.png",
    "128": "/images/backblaze128.png"
  }
}
